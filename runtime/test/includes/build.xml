<?xml version="1.0" encoding="UTF-8" ?>

<project name="avalon-test-includes" default="install" basedir="." 
    xmlns:x="antlib:org.apache.avalon.tools"
>
  <property file="build.properties"/>
  <x:home/>
  <import file="${magic.templates}/standard.xml"/>

  <target name="init" depends="standard.init">
    <x:filter key="avalon-framework-impl" feature="uri" 
       token="FRAMEWORK-IMPL-SPEC"/>
    <x:filter key="avalon-test-testa" feature="uri" 
       token="TEST-A-SPEC"/>
    <x:filter key="avalon-test-testb" feature="uri" 
       token="TEST-B-SPEC"/>
    <x:filter key="avalon-test-testc" feature="uri" 
       token="TEST-C-SPEC"/>
    <x:filter key="avalon-test-testd" feature="uri" 
       token="TEST-D-SPEC"/>
    <x:filter key="avalon-test-teste" feature="uri" 
       token="TEST-E-SPEC"/>
  </target>

</project>
