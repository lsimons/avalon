<?xml version="1.0" encoding="UTF-8" ?>

<project name="avalon-tutorial-dynamics" default="install" basedir="." 
    xmlns:x="antlib:org.apache.avalon.tools">

  <property file="build.properties"/>
  <import file="${project.system}/build/standard.xml"/>

  <x:path id="runtime"/>
  <pathconvert refid="runtime" targetos="windows" property="myprop"/>
  <echo>${myprop}</echo>

  <target name="init" depends="standard.init">
    <x:filter key="avalon-tutorial-dynamics" feature="uri" 
       token="DYNAMICS-SPEC"/>
    <x:filter key="avalon-framework-impl" feature="uri" 
       token="FRAMEWORK-IMPL-SPEC"/>
  </target>

</project>

